{
  "name": "@dxatscale/sfpowerscripts.azpipelines",
  "private": true,
  "version": "17.1.2",
  "description": "CI/CD extensions for Salesforce",
  "repository": {
    "type": "git",
    "url": "https://github.com/accenture/sfpowerscripts.git"
  },
  "keywords": [
    "vsts",
    "tfs"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "compile:tasks": "glob-exec \"build/BuildTasks/*/tsconfig.json\" -- \"tsc -b {{files.join(' ')}}\"",
    "build:copy": "node make copy",
    "build:ui": "webpack --mode production",
    "build": "rimraf build && shx mkdir build && npm run build:copy && npm run compile:tasks && npm run build:ui",
    "postbuild": "rimraf -g build/BuildTasks/**/*.ts && rimraf -g build/UI/**/*.tsx",
    "increment:azpipelines:version": "node make incrementversion",
    "package:azpipelines": "tfx extension create --root build --output-path dist --manifest-globs vss-extension.json",
    "publish:azpipelines": "node make.js publish"
  },
  "author": "dxatscale",
  "license": "MIT",
  "dependencies": {
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-node-api": "7.2.0",
    "azure-devops-ui": "^1.160.4",
    "azure-pipelines-task-lib": "^2.9.3",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "nanoid": "^2.1.11",
    "react": "~16.8.1",
    "react-dom": "~16.8.1",
    "react-virtualized": "^9.21.2",
    "sfpowerscript-installsourcepackage-task": "file:BuildTasks\\InstallSourcePackageTask",
    "sfpowerscripts-analyzewithpmd-task": "file:BuildTasks\\AnalyzeWithPMDTask",
    "sfpowerscripts-authenticateorg-task": "file:BuildTasks\\AuthenticateOrgTaskCurrent",
    "sfpowerscripts-checkoutprojectfromartifact-task": "file:BuildTasks\\CheckoutProjectFromArtifactTask",
    "sfpowerscripts-createdeltapackage-task": "file:BuildTasks\\CreateDeltaPackageTask",
    "sfpowerscripts-createsourcepackage-task": "file:BuildTasks\\CreateSourcePackageTask",
    "sfpowerscripts-createunlockedpackage-task": "file:BuildTasks\\CreateUnlockedPackageTask",
    "sfpowerscripts-deploydestructivemanifesttoorg-task": "file:BuildTasks\\DeployDestructiveManifestToOrgTask",
    "sfpowerscripts-deploysourcetoorg-task": "file:BuildTasks\\DeploySourceToOrgTask",
    "sfpowerscripts-exportsourcefromorg-task": "file:BuildTasks\\ExportSourceFromAnOrgTask",
    "sfpowerscripts-incrementprojectbuildnumber-task": "file:BuildTasks\\IncrementProjectBuildNumberTask",
    "sfpowerscripts-installpackagedependencies-task": "file:BuildTasks\\InstallPackageDependenciesTask",
    "sfpowerscripts-installsfdx-task": "file:BuildTasks\\InstallSFDXCLITaskCurrent",
    "sfpowerscripts-installunlockedpackage-task": "file:BuildTasks\\InstallUnlockedPackageTask",
    "sfpowerscripts-managescratchorg-task": "file:BuildTasks\\ManageScratchOrgTaskCurrent",
    "sfpowerscripts-promoteunlocked-task": "file:BuildTasks\\PromoteUnlockedPackageTask",
    "sfpowerscripts-triggerapextest-task": "file:BuildTasks\\TriggerApexTestTask",
    "sfpowerscripts-validateapextestcoverage-task": "file:BuildTasks\\ValidateApexCoverageTask",
    "sfpowerscripts-validatedxunlockedpackage-task": "file:BuildTasks\\ValidateDXUnlockedPackageTask",
    "sfpowerscripts-validatetestcoverage-task": "file:BuildTasks\\ValidateTestCoveragePackageTask",
    "shelljs": "^0.8.3",
    "simple-git": "2.0.0",
    "unzip-stream": "0.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.16",
    "@types/q": "^1.5.2",
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "@types/xml2js": "^0.4.5",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "~1.0.0",
    "file-loader": "~2.0.0",
    "glob-exec": "^0.1.1",
    "minimist": "^1.2.2",
    "node-sass": "~4.12.0",
    "rimraf": "^3.0.0",
    "sass-loader": "~7.1.0",
    "semver": "^6.3.0",
    "shx": "^0.3.2",
    "style-loader": "~0.23.1",
    "tfx-cli": "0.6.4",
    "ts-loader": "~5.2.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typestrict": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.1.2"
  }
}
