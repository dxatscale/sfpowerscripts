                ]
            }
        },
        {
            "id": "sfpowerscripts-salesforce-connection",
            "description": "Credentials for task invoking Salesforce using SFPowerscripts",
            "type": "ms.vss-endpoint.service-endpoint-type",
            "targets": [
                "ms.vss-endpoint.endpoint-types"
            ],
            "properties": {
                "name": "sfpowerscriptsSalesforce",
                "displayName": "Salesforce",
                "url": {
                    "displayName": "Endpoint",
                    "value": "https://login.salesforce.com/services/oauth2/token",
                    "required": true,
                    "isVisible": "true",
                    "helpText": "Url of your Salesforce Authentication Endpoint",
                    "dependsOn": {
                        "input": "environment",
                        "map": [
                            {
                                "key": "Sandbox",
                                "value": "https://test.salesforce.com/services/oauth2/token"
                            },
                            {
                                "key": "Production",
                                "value": "https://login.salesforce.com/services/oauth2/token"
                            }
                        ]
                    }
                },
                "authenticationSchemes": [
                    {
                        "type": "ms.vss-endpoint.endpoint-auth-scheme-basic",
                        "inputDescriptors": [
                            {
                                "id": "username",
                                "name": "Username",
                                "description": "Provide the user name of the User",
                                "inputMode": "textbox",
                                "isConfidential": false,
                                "validation": {
                                    "isRequired": true,
                                    "dataType": "string"
                                }
                            },
                            {
                                "id": "password",
                                "name": "Password",
                                "description": "Provide the passwword for the user",
                                "inputMode": "passwordbox",
                                "isConfidential": true,
                                "validation": {
                                    "isRequired": true,
                                    "dataType": "string"
                                }
                            },
                            {
                                "id": "securitytoken",
                                "name": "SecurityToken",
                                "description": "Provide the Security Token for the user",
                                "inputMode": "textbox",
                                "isConfidential": true,
                                "validation": {
                                    "isRequired": false,
                                    "dataType": "string"
                                }
                            },
                            {
                                "id": "environment",
                                "name": "Environment",
                                "description": "Environment Type",
                                "inputMode": "combo",
                                "isConfidential": false,
                                "validation": {
                                    "isRequired": true,
                                    "dataType": "string",
                                    "maxLength": 300
                                },
                                "values": {
                                    "inputId": "environmentValues",
                                    "defaultValue": "AzureCloud",
                                    "possibleValues": [
                                        {
                                            "value": "Production",
                                            "displayValue": "Production/DevHub"
                                        },
                                        {
                                            "value": "Sandbox",
                                            "displayValue": "Sandbox"
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                ]
            }
        },
