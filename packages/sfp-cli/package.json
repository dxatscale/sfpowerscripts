{
  "name": "@dxatscale/sfp-cli",
  "description": "Developer CLI for Salesforce programs following the DX@Scale model",
  "version": "0.0.45",
  "author": "dxatscale",
  "bin": {
    "sfp": "./bin/run"
  },
  "bugs": "https://github.com/dxatscale/sfp-cli/issues",
  "dependencies": {
    "@dxatscale/sfpowerscripts.core": "^15.2.9",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/parser": "^3.8.5",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/plugin-update": "^1.5.0",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@salesforce/core": "2.35.0",
    "@salesforce/source-deploy-retrieve": "5.9.5",
    "adm-zip": "^0.5.9",
    "async-retry": "^1.3.3",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.6",
    "fs-extra": "^10.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.1.2",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "lodash": "^4.17.21",
    "neverthrow": "^4.2.2",
    "simple-git": "^2.45.0",
    "tslib": "^1.14.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.21",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^7.3.3",
    "@types/inquirer-autocomplete-prompt": "^1.3.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dxatscale/sfp-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfp",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 0,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>. Use <%= chalk.greenBright('npm update --global sfdx-cli') %> to update to the latest version."
    }
  },
  "repository": "dxatscale/sfp-cli",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf ./lib && rimraf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "prepack": "oclif-dev manifest && npm shrinkwrap"
  },
  "types": "lib/index.d.ts"
}
